<library path="lib/libtuw_nav_rviz">
  <class name="tuw_nav_rviz/Spline"
         type="tuw_nav_rviz::SplineDisplay"
         base_class_type="rviz::Display">
    <description>
      Displays a B-Spline (up to 3d)
    </description>
    <message_type>tuw_nav_msgs/Spline</message_type>
  </class>
  <class name="tuw_nav_rviz/RouteSegments"
         type="tuw_nav_rviz::RouteSegmentsDisplay"
         base_class_type="rviz::Display">
    <description>
      Displays RouteSegments
    </description>
    <message_type>tuw_nav_msgs/RouteSegments</message_type>
  </class>
  <class name="tuw_nav_rviz/Path"
         type="tuw_nav_rviz::PathDisplay"
         base_class_type="rviz::Display">
    <description>
      Displays RouteSegments
    </description>
    <message_type>nav_msgs/Path</message_type>
  </class>
</library>

